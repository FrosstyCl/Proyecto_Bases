<%include partials/header%>

<div class="container">
        <div class="row mt-5"> 
            <div class="col-md-7">
                    <table class="table" >
                        <thead class="thead-dark">
                          <tr>
                            <th scope="col" >Comuna</th>
                            <th scope="col" >Doctores</th>
                          </tr>
                        </thead>
                        <tbody>
                        <%data.forEach(function(comuna){%>
                            <tr>
                            <td><%=comuna.comuna%></td>
                            <td><%=comuna.cant%></td>
                            <%});%>
                        </tr>
                        </tbody>
                      </table>
            </div>
            <div class="col-md-5">
              <div class="card">
                <div class="card-body">
                  <form action="/consulta" method="POST">
                    <input type="radio" name="consulta" value="1" id="">Paciente que mas ha gastado<br>
                    <input type="radio" name="consulta" value="2" id="">Doctor que menos ha ganado <br>
                    <input type="radio" name="consulta" value="3" id="">Dia que se hicieron mas atenciones <br>
                    <input type="radio" name="consulta" value="4" id="">Mejor Doctor de cada especialidad(Mas atenciones)<br>
                    <input type="radio" name="consulta" value="5" id="">Cantidad de Doctores por Comuna <br>
                    <input type="submit"  class="btn btn-primary" value="Consultar">
                    <a href="/consultas.html" class="btn btn-dark"> Regresar</a>
                  </form>
                </div>

<%include partials/footer%>